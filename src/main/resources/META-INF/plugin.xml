<idea-plugin>
  <id>io.github.v-zacek.phpcore-support</id>
  <name>Phpcore support</name>
  <vendor url = "https://github.com/v-zacek/">v-zacek</vendor>
  <version>1.0.0</version>

  <idea-version since-build = "250" until-build = "252.*"/>

  <depends>com.intellij.modules.platform</depends>
  <depends>com.jetbrains.php</depends>

  <extensions defaultExtensionNs = "com.intellij">
    <psi.referenceContributor implementation = "LogicClassReference.LogicClassReferenceContributor"/>
    <psi.referenceContributor implementation = "FormFunctionReference.FormFunctionReferenceContributor"/>
    <psi.referenceContributor implementation = "LogicClassFunctionReference.LogicClassFunctionReferenceContributor"/>
  </extensions>
</idea-plugin>
